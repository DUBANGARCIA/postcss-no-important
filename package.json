{
	"name": "postcss-no-important",
	"version": "10.0.7",
	"description": "PostCSS plugin for delete declarations !important",
	"keywords": [
		"postcss",
		"css",
		"postcss-plugin",
		"important"
	],
	"homepage": "https://github.com/DUBANGARCIA/postcss-no-important",
	"bugs": {
		"url": "https://github.com/DUBANGARCIA/postcss-no-important/issues"
	},
	"repository": "DUBANGARCIA/postcss-no-important",
	"license": "MIT",
	"author": "Duban Garcia <duban162008@hotmail.com>",
	"main": "src/index.js",
	"scripts": {
		"commit": "yarn git-cz",
		"postinstall": "yarn sort-package-json",
		"lint": "yarn eslint",
		"prepare": "yarn is-ci || yarn husky install",
		"release": "yarn release-it --ci",
		"test": "yarn jest"
	},
	"config": {
		"commitizen": {
			"path": "git-cz"
		}
	},
	"devDependencies": {
		"@babel/core": "^7.21.8",
		"@babel/preset-env": "^7.21.5",
		"@commitlint/cli": "^17.6.3",
		"@commitlint/config-conventional": "^17.6.3",
		"@release-it/bumper": "^4.0.2",
		"@release-it/conventional-changelog": "^5.1.1",
		"clean-publish": "^4.2.0",
		"commitlint-plugin-function-rules": "^1.7.1",
		"eslint": "^8.40.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-ban": "^1.6.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-jest": "^27.2.1",
		"eslint-plugin-jsdoc": "^46.2.6",
		"eslint-plugin-no-null": "^1.0.2",
		"eslint-plugin-no-secrets": "^0.8.9",
		"eslint-plugin-no-unsanitized": "^4.0.2",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prefer-arrow": "^1.2.3",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-security": "^1.7.1",
		"eslint-plugin-unicorn": "^47.0.0",
		"git-cz": "^4.9.0",
		"husky": "^8.0.3",
		"is-ci": "^3.0.1",
		"jest": "^29.5.0",
		"lint-staged": "^13.2.2",
		"postcss": "^8.4.23",
		"prettier": "^2.8.8",
		"release-it": "^15.10.3",
		"sort-package-json": "^2.4.1",
		"yarnhook": "^0.6.0"
	},
	"peerDependencies": {
		"postcss": "^8.4.19"
	},
	"packageManager": "yarn@3.2.4",
	"publishConfig": {
		"directory": "dist"
	},
	"clean-publish": {
		"withoutPublish": true,
		"tempDir": "dist",
		"packageManager": "yarn",
		"files": [
			".yarn",
			".idea",
			".husky",
			".huskyrc.js",
			".yarnrc.yml",
			"dependabot.yml",
			"lint-staged.config.js"
		]
	}
}
