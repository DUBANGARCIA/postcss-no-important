{
  "name": "postcss-no-important",
  "version": "10.0.7",
  "description": "PostCSS plugin for delete declarations !important",
  "keywords": ["postcss", "css", "postcss-plugin", "important"],
  "homepage": "https://github.com/DUBANGARCIA/postcss-no-important",
  "bugs": {
    "url": "https://github.com/DUBANGARCIA/postcss-no-important/issues"
  },
  "repository": "DUBANGARCIA/postcss-no-important",
  "license": "MIT",
  "author": "Duban Garcia <duban162008@hotmail.com>",
  "main": "src/index.js",
  "scripts": {
    "commit": "pnpm git-cz",
    "postinstall": "pnpm sort-package-json",
    "prepare": "! ./node_modules/.bin/is-ci && pnpx husky",
    "release": "pnpm release-it --ci",
    "test": "pnpm jest"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "devDependencies": {
    "@babel/core": "7.24.5",
    "@babel/preset-env": "7.24.5",
    "@biomejs/biome": "1.7.3",
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@jest/globals": "29.7.0",
    "@release-it/bumper": "6.0.1",
    "@release-it/conventional-changelog": "8.0.1",
    "babel-jest": "29.7.0",
    "clean-publish": "4.4.0",
    "git-cz": "4.9.0",
    "husky": "9.0.11",
    "is-ci": "3.0.1",
    "jest": "29.7.0",
    "lint-staged": "15.2.4",
    "release-it": "17.3.0",
    "sort-package-json": "2.10.0",
    "yarnhook": "0.6.2"
  },
  "peerDependencies": {
    "postcss": "^8.4.38"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "clean-publish": {
    "withoutPublish": true,
    "tempDir": "dist",
    "files": [".idea", ".husky", "lint-staged.config.js"]
  }
}
